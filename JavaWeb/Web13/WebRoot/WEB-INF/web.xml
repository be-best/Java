<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>Web13</display-name>
  <servlet>
    <servlet-name>JSPServlet</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>bull02.JSPdemo2.JSPServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountServlet</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>bull04.transfer.AccountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountServlet_1</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>bull05.transfer_1.AccountServlet_1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountServlet_2</servlet-name>
